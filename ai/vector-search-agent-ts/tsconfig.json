{
    "name": "ts-cosmos-nodejs-mongo-vcore-vector-agent-samples",
    "version": "1.0.0",
    "description": "TypeScript agent samples for MongoDB vCore vector search with Cosmos DB",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "agent": "node --env-file .env dist/agent.js"
    },
    "compilerOptions": {
        "target": "ES2020",
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}